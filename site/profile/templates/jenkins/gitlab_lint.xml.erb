<?xml version="1.0" encoding="UTF-8"?><project>
  <actions/>
  <description>Performs puppet lint tests on the <%=@test_module_name-%> module. Triggers in response to a gitlab webhook, using the gitlab-webhook plugin.</description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@2.3.2">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
	      <url><%= @gitlab_url -%>/<%=@gitlab_username-%>/<%=@test_module_name-%>.git</url>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/master</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <browser class="hudson.plugins.git.browser.GitLab">
	    <url><%= @gitlab_url -%>/<%=@gitlab_username-%>/<%=@test_module_name-%></url>
      <version>7.6</version>
    </browser>
    <submoduleCfg class="list"/>
    <extensions/>
  </scm>
  <assignedNode>swarm</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <customWorkspace><%=@test_module_name-%></customWorkspace>
  <builders>
    <hudson.tasks.Shell>
      <command>puppet-lint manifests/init.pp</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>
